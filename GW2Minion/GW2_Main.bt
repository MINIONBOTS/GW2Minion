ed = GUI:Keybind(\"##stepBackwardsKey\", Settings.GW2Minion[acc_name].stepBackwardsKey)\n      if changed then\n         Settings.GW2Minion[acc_name].stepBackwardsKey = content\n         Settings.GW2Minion.stepBackwardsKey = content\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      EndLabel(GetString(\"Your ingame keybind to move backwards. Arrow keys do not work! Check the ingame settings to find your keybind (F11).\"))\n\n      StartLabel(GetString(\"Springer: High Vault\"))\n      content, changed = GUI:Checkbox(\"##springerMastered\", Settings.GW2Minion[acc_name].springerMastered)\n      if changed then\n         Settings.GW2Minion[acc_name].springerMastered = content\n         Settings.GW2Minion.springerMastered = content\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      EndLabel(GetString(\"Only check this if you unlocked the mastery 'High Vault' for your springer mount.\"))\n\n      StartLabel(GetString(\"Jackal: Shifting Sands\"))\n      content, changed = GUI:Checkbox(\"##jackalPortalMastered\", Settings.GW2Minion[acc_name].jackalPortalMastered)\n      if changed then\n         Settings.GW2Minion[acc_name].jackalPortalMastered = content\n         Settings.GW2Minion.jackalPortalMastered = content\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      EndLabel(GetString(\"Only check this if you unlocked the mastery 'Shifting Sands' for your jackal mount.\"))\n\n      StartLabel(GetString(\"Raptor: Canyon Jump\"))\n      content, changed = GUI:Checkbox(\"##raptorMastered\", Settings.GW2Minion[acc_name].raptorMastered)\n      if changed then\n         Settings.GW2Minion[acc_name].raptorMastered = content\n         Settings.GW2Minion.raptorMastered = content\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      EndLabel(GetString(\"Only check this if you unlocked the mastery 'Canyon Jumping' for your raptor mount.\"))\n  