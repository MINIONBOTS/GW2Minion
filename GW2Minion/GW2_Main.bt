ction to skip ..yet...\n      Settings.GW2Minion.vendorassist = GUI:Checkbox(GetString(\"Show Vendor Assistant\"), Settings.GW2Minion.vendorassist)\n      Settings.GW2Minion.claimRewards = GUI:Checkbox(GetString(\"Claim Rewards\"), Settings.GW2Minion.claimRewards)\n      Settings.GW2Minion.equipGathering = GUI:Checkbox(GetString(\"Equip Gathering Tools\"), Settings.GW2Minion.equipGathering)\n      Settings.GW2Minion.Loot = GUI:Checkbox(GetString(\"Loot\"), Settings.GW2Minion.Loot)\n      Settings.GW2Minion.salvage = GUI:Checkbox(GetString(\"Run Salvage Manager\"), Settings.GW2Minion.salvage)\n      Settings.GW2Minion.deposititems = GUI:Checkbox(GetString(\"Deposit items\"), Settings.GW2Minion.deposititems)\n      Settings.GW2Minion.combatmovement = GUI:Checkbox(GetString(\"Combat movement\"), Settings.GW2Minion.combatmovement)\n      Settings.GW2Minion.finishenemyassist = GUI:Checkbox(GetString(\"Auto stomp\"), Settings.GW2Minion.finishenemyassist)\n      Settings.GW2Minion.ignoreyellowmobs = GUI:Checkbox(GetString(\"Ignore yellow enemies\"), Settings.GW2Minion.ignoreyellowmobs)\n      Settings.GW2Minion.skipcutscene = GUI:Checkbox(GetString(\"Skip cutscene\"), Settings.GW2Minion.skipcutscene)\n      local changed\n      Settings.GW2Minion.reduceram, changed = GUI:Checkbox(GetString(\"Reduce RAM usage\"), Settings.GW2Minion.reduceram)\n      if (changed) then\n         ClearMemory()\n      end\n      if (GUI:IsItemHovered()) then\n         GUI:SetTooltip(GetString(\"If enabled, GW2 uses less RAM at the cost of higher CPU and loading times (HDD).\"))\n      end\n      Settings.GW2Minion.disablerendering, changed = GUI:Checkbox(GetString(\"Disable rendering\"), Settings.GW2Minion.disablerendering)\n      if (changed) then\n         RenderManager:ToggleRendering(Settings.GW2Minion.disablerendering)\n      end\n      if (GUI:IsItemHovered()) then\n         GUI:SetTooltip(GetString(\"Turns off Rendering. Can cause crashes when you have the Main-Map open in the wrong moment;)\"))\n      end\n      GUI:PushItemWidth(100)\n      Settings.GW2Minion.evadehpthreshold = GUI:SliderInt(GetString(\"Evade HP\"), Settings.GW2Minion.evadehpthreshold, 0, 100)\n      if (GUI:IsItemHovered()) then\n         GUI:SetTooltip(GetString(\"The Bot will evade attacks if your HP Percent is below this value.\"))\n      end\n      GUI:PopItemWidth()\n      GUI:SetNextTreeNodeOpened(true, GUI.SetCond_Once)\n      GUI:SetNextTreeNodeOpened(false, GUI.SetCond_Appearing)\n      if (GUI:TreeNode(GetString(\"Revive\"))) then\n         Settings.GW2Minion.revivedeadplayers = GUI:Checkbox(GetString(\"Revive dead players\"), Settings.GW2Minion.revivedeadplayers)\n         Settings.GW2Minion.revivedownedplayers = GUI:Checkbox(GetString(\"Revive downed players\"), Settings.GW2Minion.revivedownedplayers)\n         Settings.GW2Minion.revivenpcs = GUI:Checkbox(GetString(\"Revive NPCs\"), Settings.GW2Minion.revivenpcs)\n         Settings.GW2Minion.revivepartymember = GUI:Checkbox(GetString(\"Revive party members\"), Settings.GW2Minion.revivepartymember)\n         GUI:TreePop()\n      end\n      GUI:TreePop()\n   end\n\n   -- Mount\n   if (GUI:TreeNode(GetString(\"Mounts\"))) then\n      local changed\n      Settings.GW2Minion[acc_name].usemount, changed = GUI:Checkbox(GetString(\"Use Mount\"), Settings.GW2Minion[acc_name].usemount)\n      if changed then\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      Settings.GW2Minion[acc_name].mountAbility2Key, changed = GUI:Keybind(\"Mount Ability 1 Key\", Settings.GW2Minion[acc_name].mountAbility2Key)\n      if changed then\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      Settings.GW2Minion[acc_name].stepBackwardsKey = GUI:Keybind(GetString(\"Backward walk key\"), Settings.GW2Minion[acc_name].stepBackwardsKey)\n      if changed then\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      Settings.GW2Minion[acc_name].springerMastered = GUI:Checkbox(GetString(\"Springer high jump mastered\"), Settings.GW2Minion[acc_name].springerMastered)\n      if changed then\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      Settings.GW2Minion[acc_name].jackalPortalMastered = GUI:Checkbox(GetString(\"Jackal sand portal mastered\"), Settings.GW2Minion[acc_name].jackalPortalMastered)\n      if changed then\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      Settings.GW2Minion[acc_name].raptorMastered = GUI:Checkbox(GetString(\"Raptor long jump mastered\"), Settings.GW2Minion[acc_name].raptorMastered)\n      if changed then\n         Settings.GW2Minion[acc_name] = Settings.GW2Minion[acc_name]\n      end\n      GUI:TreePop()\n   end\n\n   -- Character Select\n   if (GUI:TreeNode(GetString(\"Character select screen\"))) then\n      Settings.GW2Minion.autostartbot = GUI:Checkbox(GetString(\"Auto start bot\"), Settings.GW2Minion.autostartbot)\n      GUI:PushItemWidth(150)\n      Settings.GW2Minion.autostart_delay = GUI:InputInt(GetString(\"Auto start delay (ms)\"), Settings.GW2Minion.autostart_delay, 250, 1000)\n      GUI:PopItemWidth()\n\n      GUI:PushItemWidth(150)\n      Settings.GW2Minion.guestserver = GUI:Combo(GetString(\"Guest server\"), Settings.GW2Minion.guestserver, ml_global_information.GuestServerList)\n      GUI:PopItemWidth()\n      GUI:TreePop()\n   end\n\n   GUI:TreePop()\nend\n-- Render Skill Proile UI Stuff\nif (SkillManager) then\n   SkillManager:RenderCodeEditor()\nend",
