		info = "GW2Minion core code, gets called from gw2.lua every Tick.\nUpdates Managers and sets Global Variables. Handles GameStates.\nUpdates Managers and global variables. Handles Main Window and GameStates.",
		menucode = "-- Character Select\nif (Settings.GW2Minion.autostartbot == nil) then\n   Settings.GW2Minion.autostartbot = false\nend\nif (Settings.GW2Minion.guestserver == nil) then\n   Settings.GW2Minion.guestserver = 0\nend\n\n-- Ingame\nif (Settings.GW2Minion.vendorassist == nil) then\n   Settings.GW2Minion.vendorassist = true\nend\nif (Settings.GW2Minion.claimRewards == nil) then\n   Settings.GW2Minion.claimRewards = true\nend\nif (Settings.GW2Minion.equipGathering == nil) then\n   Settings.GW2Minion.equipGathering = true\nend\nif (Settings.GW2Minion.Loot == nil) then\n   Settings.GW2Minion.Loot = true\nend\nif (Settings.GW2Minion.salvage == nil) then\n   Settings.GW2Minion.salvage = true\nend\nif (Settings.GW2Minion.deposititems == nil) then\n   Settings.GW2Minion.deposititems = true\nend\nif (Settings.GW2Minion.combatmovement == nil) then\n   Settings.GW2Minion.combatmovement = true\nend\nif (Settings.GW2Minion.evadehpthreshold == nil) then\n   Settings.GW2Minion.evadehpthreshold = 80\nend\nif (Settings.GW2Minion.ignoreyellowmobs == nil) then\n   Settings.GW2Minion.ignoreyellowmobs = false\nend\nif (Settings.GW2Minion.revivedownedplayers == nil) then\n   Settings.GW2Minion.revivedownedplayers = true\nend\nif (Settings.GW2Minion.revivedeadplayers == nil) then\n   Settings.GW2Minion.revivedeadplayers = false\nend\nif (Settings.GW2Minion.revivenpcs == nil) then\n   Settings.GW2Minion.revivenpcs = false\nend\nif (Settings.GW2Minion.revivepartymember == nil) then\n   Settings.GW2Minion.revivepartymember = true\nend\nif (Settings.GW2Minion.skipcutscene == nil) then\n   Settings.GW2Minion.skipcutscene = false\nend\nif (Settings.GW2Minion.reduceram == nil) then\n   Settings.GW2Minion.reduceram = false\nend\nif (Settings.GW2Minion.disablerendering == nil) then\n   Settings.GW2Minion.disablerendering = false\nend\nif (Settings.GW2Minion.finishenemyassist == nil) then\n   Settings.GW2Minion.finishenemyassist = true\nend\n\n-- Mounts\nlocal acc_name = GetAccountName()\nif