		filename = "GW2_Main.bt",
		finishcode = "",
		info = "GW2Minion core code, gets called from gw2.lua every Tick.\nUpdates Managers and sets Global Variables. Handles GameStates.\nUpdates Managers and global variables. Handles Main Window and GameStates.",
		menucode = "local visible, open = Settings.GW2Minion.UI.visible, Settings.GW2Minion.UI.open\nif open and visible then\n   visible, open = GUI:Begin(\"GW2Minion Settings\", open)\n   GUI:PushItemWidth(-1)\n   local function StartLabel(txt)\n      GUI:BeginGroup()\n      GUI:AlignFirstTextHeightToWidgets()\n      GUI:Text(GetString(txt))\n      GUI:SameLine(165)\n   end\n   local function EndLabel(tooltip)\n      GUI:EndGroup()\n\n      if tooltip and (GUI:IsItemHovered()) then\n         GUI:SetTooltip(tooltip)\n      end\n   end\n   local acc_name = GetAccountName()\n   local content, changed\n   if GUI:CollapsingHeader(\"General\") then\n      StartLabel(GetString(\"Reduce RAM usage\"))\n      content, changed = GUI:Checkbox(\"##reduceram\", Settings.GW2Minion.reduceram)\n      if (changed) then\n         Settings.GW2Minion.reduceram = content\n         ClearMemory()\n      end\n      EndLabel(GetString(\"If enabled, GW2 uses less RAM at the cost of higher CPU and loading times (HDD).\"))\n\n      StartLabel(GetString(\"Disable rendering\"))\n      content, changed = GUI:Checkbox(\"##disablerendering\", Settings.GW2Minion.disablerendering)\n      if (changed) then\n         Settings.GW2Minion.disablerendering = content\n         RenderManager:ToggleRendering(Settings.GW2Minion.disablerendering)\n      end\n      EndLabel(GetString(\"Turns off Rendering. Can cause crashes when you have the Main-Map open in the wrong moment!\"))\n\n      StartLabel(GetString(\"Auto start bot\"))\n      content, changed = GUI:Checkbox(\"##autostartbot\", Settings.GW2Minion.autostartbot)\n      if changed then\n         Settings.GW2Minion.autostartbot = content\n      end\n      EndLabel(GetString(\"Automatically start the bot one time after the set delay.\"))\n\n      StartLabel(GetString(\"Auto start delay (ms)\"))\n      content, changed = GUI:InputInt(\"##autostart_delay\", Settings.GW2Minion.autostart_delay, 250, 1000)\n      if changed then\n         Settings.GW2Minion.autostart_delay = content\n      end\n      EndLabel(GetString(\"Set the delay before starting the bot.\"))\n\n      StartLabel(GetString(\"Guest server\"))\n      content, changed = GUI:Combo(\"##guestserver\", Settings.GW2Minion.guestserver, ml_global_information.GuestServerList)\n      if changed then\n         Settings.GW2Minion.guestserver = content\n      end\n      EndLabel(GetString(\"Select the server you want to guest on. This won't change much since the change made by anet with their mega servers.\"))\n   end\n\n   if GUI:CollapsingHeader(\"Ingame\") then\n\n      StartLabel(GetString(\"Show Vendor Assistant\"))\n      content, changed = GUI:Checkbox(\"##vendorassist\", Settings.GW2Minion.vendorassist)\n      if changed then\n         Settings.GW2Minion.vendorassist = content\n      end\n      EndLabel(GetString(\"Toggles the vendor assistant window.\"))\n\n      StartLabel(GetString(\"Claim Rewards\"))\n      content, changed = GUI:Checkbox(\"##claimRewards\", Settings.GW2Minion.claimRewards)\n      if changed then\n         Settings.GW2Minion.claimRewards = content\n      end\n      EndLabel(GetString(\"Automatically claim rewards if possible.\"))\n\n      StartLabel(GetString(\"Equip Gathering Tools\"))\n      content, changed = GUI:Checkbox(\"##equipGathering\", Settings.GW2Minion.equipGathering)\n      if changed then\n         Settings.GW2Minion.equipGathering = content\n      end\n      EndLabel(GetString(\"Automatically equip gathering tools if none are equipped.\"))\n\n      StartLabel(GetString(\"Loot\"))\n      content, changed = GUI:Checkbox(\"##Loot\", Settings.GW2Minion.Loot)\n      if changed then\n         Settings.GW2Minion.Loot = content\n      end\n      EndLabel(GetString(\"Automatically loot all nearby enemies.\"))\n\n      StartLabel(GetString(\"Run Salvage Manager\"))\n      content, changed = GUI:Checkbox(\"##salvage\", Settings.GW2Minion.salvage)\n      if changed then\n         Settings.GW2Minion.salvage = content\n      end\n      EndLabel(GetString(\"Use the 'Salvage Manager' to automatically salvage gear. Requires to set up said Manager.\"))\n\n      StartLabel(GetString(\"Deposit items\"))\n      content, changed = GUI:Checkbox(\"##deposititems\", Settings.GW2Minion.deposititems)\n      if changed then\n         Settings.GW2Minion.deposititems = content\n      end\n      EndLabel(GetString(\"Automatically deposit materials in your bank every 16 seconds.\"))\n\n      StartLabel(GetString(\"Deposit items\"))\n      content, changed = GUI:Checkbox(\"##deposititems\", Settings.GW2Minion.deposititems)\n      if changed then\n         Settings.GW2Minion.deposititems = content\n      end\n      EndLabel(GetString(\"Automatically deposit materials in your bank every 16 seconds.\"))\n\n      StartLabel(GetString(\"Skip cutscene\"))\n      content, changed = GUI:Checkbox(\"##skipcutscene\", Settings.GW2Minion.skipcutscene)\n      if changed then\n         Settings.GW2Minion.skipcutscene = content\n      end\n      EndLabel(GetString(\"Try to skip cutscences automatically.\"))\n   end\n\n   if GUI:CollapsingHeader(\"Revive\") then\n\n      StartLabel(GetString(\"Revive dead players\"))\n      content, changed = GUI:Checkbox(\"##revivedeadplayers\", Settings.GW2Minion.revivedeadplayers)\n      if changed then\n         Settings.GW2Minion.revivedeadplayers = content\n      end\n      EndLabel(GetString(\"Revive dead players if your current botmode uses the default revive handler.\"))\n\n      StartLabel(GetString(\"Revive downed players\"))\n      content, changed = GUI:Checkbox(\"##revivedownedplayers\", Settings.GW2Minion.revivedownedplayers)\n      if changed then\n         Settings.GW2Minion.revivedownedplayers = content\n      end\n      EndLabel(GetString(\"Revive downed players if your current botmode uses the default revive handler.\"))\n\n      StartLabel(GetString(\"Revive NPCs\"))\n      content, changed = GUI:Checkbox(\"##revivenpcs\", Settings.GW2Minion.revivenpcs)\n      if changed then\n      